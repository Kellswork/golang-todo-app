{
  "compilerOptions": {
    "target": "es2017",
    "module": "ESNext",
    "sourceMap": true,
    "strict": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "allowUnreachableCode": false,
    "strictFunctionTypes": true,
    "noUnusedParameters": true,
    "noImplicitReturns": false
  }
}